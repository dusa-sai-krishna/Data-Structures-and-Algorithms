   "source": [
    "def ToH(n,a,b,c):\n",
    "    if n==0:\n",
    "        return \n",
    "    if n==1:\n",
    "        print(\"move 1st the disk from\",a,\"to\",c)\n",
    "        return\n",
    "    ToH(n-1,a,c,b)\n",
    "    print(\"move\",n,\"th disk from \",a,\"to\",c)\n",
    "    ToH(n-1,b,a,c)"
   ]
  },
