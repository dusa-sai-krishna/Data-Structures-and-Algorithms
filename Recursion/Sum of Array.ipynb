   "source": [
    "def sum_array(arr):\n",
    "    if(len(arr)==0):\n",
    "        return 0\n",
    "    elif (len(arr)==1):\n",
    "        return arr[0]\n",
    "    else:\n",
    "        return arr[0]+sum_array(arr[1:])"
   ]
  },
