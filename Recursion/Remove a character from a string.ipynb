   "source": [
    "def remove(str,ch):\n",
    "    if (len(str)==0):\n",
    "        return str\n",
    "    smallstr=remove(str[1:],ch)\n",
    "    if(str[0]==ch):\n",
    "        return smallstr\n",
    "    else:\n",
    "        return str[0]+smallstr"
   ]
