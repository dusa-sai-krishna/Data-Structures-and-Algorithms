   "source": [
    "def replace(str):\n",
    "    if len(str)==0:\n",
    "        return str\n",
    "    smallstr=replace(str[1:])\n",
    "    if str[0]=='a':\n",
    "        return 'b'+smallstr\n",
    "    else:\n",
    "        return str[0]+smallstr"
   ]
