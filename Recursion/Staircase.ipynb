    "def count(n):\n",
    "    if n==0 or n==1:\n",
    "        return 1\n",
    "    elif n<0:\n",
    "        return 0\n",
    "    else:\n",
    "        return count(n-3)+count(n-2)+count(n-1)\n",
    "    \n",
    "print(count(int(input())))"
