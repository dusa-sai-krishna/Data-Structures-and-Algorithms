    "def printSubseq(str,ans):\n",
    "    if len(str)==0:\n",
    "        print(ans)\n",
    "        return\n",
    "    #including\n",
    "    printSubseq(str[1:],ans+str[0])\n",
    "    #not including\n",
    "    printSubseq(str[1:],ans)"
