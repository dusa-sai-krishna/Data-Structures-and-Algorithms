   "source": [
    "def count_steps(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    if n==1:\n",
    "        return n\n",
    "    if n==2:\n",
    "        return n\n",
    "    return count_steps(n-1)+count_steps(n-2)+count_steps(n-3)"
   ]
