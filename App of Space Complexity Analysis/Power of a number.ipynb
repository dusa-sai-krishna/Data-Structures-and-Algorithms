   "source": [
    "def power(x,n):\n",
    "    if n==0 and x==0:\n",
    "        return 0\n",
    "    elif n==0 and x!=0:\n",
    "        return 1\n",
    "    else:\n",
    "        flag=n%2\n",
    "        if flag==0:\n",
    "            return x**2 *power(x,n-2)\n",
    "        else:\n",
    "            return x*power(x,n-1)\n",
    "    "
   ]
